<Application x:Class="Sample.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Sample.App"
             xmlns:converters="clr-namespace:Sample.App.Converters"
             StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converters:NullOrEmptyToIsEnabledConverter x:Key="NullOrEmptyToIsEnabledConverter"/>
            <local:ViewModelLocator x:Key="ViewModelLocator" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
